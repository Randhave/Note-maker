<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>saveNotes</servlet-name>
		<display-name>saveNotes</display-name>
		<description></description>
		<servlet-class>com.servlets.saveNotes</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>deleteNotes</servlet-name>
		<display-name>deleteNotes</display-name>
		<description></description>
		<servlet-class>com.servlets.deleteNotes</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>updateNotes</servlet-name>
		<display-name>updateNotes</display-name>
		<description></description>
		<servlet-class>com.servlets.updateNotes</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>saveNotes</servlet-name>
		<url-pattern>/saveNotes</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>deleteNotes</servlet-name>
		<url-pattern>/deleteNotes</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>updateNotes</servlet-name>
		<url-pattern>/updateNotes</url-pattern>
	</servlet-mapping>

 
</web-app>
